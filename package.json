{
	"name": "frigg",
	"type": "module",
	"author": "Rune Klein (@rnkln)",
	"description": "Mono repo containing project configs",
	"version": "1.0.0",
	"license": "MIT",
	"private": true,
	"packageManager": "yarn@3.3.0",
	"repository": {
		"type": "git",
		"url": "git@github.com:klnjs/frigg.git"
	},
	"workspaces": [
		"packages/*"
	],
	"scripts": {
		"sync": "yarn node ./scripts/cli sync",
		"lint": "eslint ./packages --cache --max-warnings 0",
		"code": "prettier ./packages --write --cache --cache-location=./.prettiercache",
		"build": "yarn node ./scripts/cli build"
	},
	"devDependencies": {
		"@slack/webhook": "^6.1.0",
		"eslint": "^8.29.0",
		"eslint-plugin-import": "^2.26.0",
		"find-up": "^6.3.0",
		"prettier": "^2.8.1",
		"rmfr": "^2.0.0",
		"simple-git": "^3.15.1",
		"yargs": "^17.6.2"
	}
}
