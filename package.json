{
	"name": "frigg",
	"type": "module",
	"author": "Rune Klein (@rnkln)",
	"description": "Mono repo containing project configs",
	"version": "0.0.2",
	"license": "MIT",
	"private": true,
	"packageManager": "yarn@3.3.0",
	"repository": {
		"type": "git",
		"url": "git@github.com:klnjs/frigg.git"
	},
	"workspaces": [
		"packages/*"
	],
	"scripts": {
		"sync": "yarn node ./scripts/cli sync",
		"bump": "yarn node ./scripts/cli bump",
		"verify": "yarn node ./scripts/cli verify",
		"publish": "yarn node ./scripts/cli publish",
		"lint": "eslint ./packages --cache --max-warnings 0",
		"code": "prettier ./packages --write --cache --cache-location=./.prettiercache"
	},
	"devDependencies": {
		"eslint": "^8.29.0",
		"eslint-plugin-import": "^2.26.0",
		"find-up": "^6.3.0",
		"inquirer": "^9.1.4",
		"prettier": "^2.8.1",
		"yargs": "^17.6.2"
	}
}
